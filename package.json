{
    "name": "ZapWebCommon",
    "version": "0.1.1",
    "private": true,
    "dependencies": {
        "@babel/runtime": "7.0.0-beta.55",
        "@material-ui/core": "^3.5.1",
        "@material-ui/docs": "^3.0.0-alpha.9",
        "@material-ui/icons": "^3.0.1",
        "@material-ui/utils": "^3.0.0-alpha.0",
        "formik": "^1.3.1",
        "gh-pages": "^2.0.1",
        "lodash-es": "^4.17.11",
        "mime": "^2.3.1",
        "namor": "^1.0.1",
        "normalize.css": "^8.0.0",
        "react": "16.4.0",
        "react-dom": "^16.3.2",
        "react-dropzone": "^7.0.1",
        "react-motion": "^0.5.2",
        "react-router-dom": "^4.2.2",
        "react-scripts": "^1.1.4",
        "react-table": "^6.8.6",
        "react-textarea-autosize": "^7.0.4",
        "react-vis": "^1.10.0",
        "stylus": "^0.54.5",
        "yup": "^0.26.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "css": "stylus -w src/stylus/index.styl -o src",
        "build-lib-css": "stylus src/stylus/base.styl -o lib/css",
        "build-lib-js": "npx babel src/components --out-dir lib/js",
        "dev": "gulp",
        "deploy": "gh-pages -d build"
    },
    "homepage": "https://chaozhang.github.io/Material-UI-React/",
    "files": [
        "lib"
    ],
    "babel": {
        "presets": [
            "env",
            "react"
        ],
        "plugins": [
            [
                "transform-object-rest-spread",
                {
                    "useBuiltIns": true
                }
            ],
            [
                "transform-runtime",
                {
                    "helpers": false,
                    "polyfill": false,
                    "regenerator": true,
                    "moduleName": "babel-runtime"
                }
            ]
        ]
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "gulp": "^3.9.1",
        "gulp-exec": "^3.0.2",
        "npx": "^10.2.0"
    }
}
