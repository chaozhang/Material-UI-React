.page-icons
	h3
		margin-bottom 20px

	ul
		display flex
		flex-flow wrap
		overflow scroll

		li
			cursor pointer
			padding 10px
			border color-white 1px solid
			display flex
			height fit-content

			&:hover
				background-color color-blue-sparta
				border color-blue-rome 1px solid

			svg
				width 40px
				height 40px
				fill color-blue-aluminium

.modal-icon-title
	display flex
	align-items center
	justify-content space-between

	svg
		width 40px
		height 40px
		fill color-blue-aluminium

